
    <form action="/contact/update" method="POST">
    <input type="hidden" name="id" value="<%=contactUpdate.id %>">

    Name: <input type="text" name="name" value="<%=contactUpdate.name %>">
    </br>
    Address  :<input type="text" name="address" value="<%=contactUpdate.address%>">
    </br>
    Email:<input type="text" name="email" id="email" value="<%=contactUpdate.email%>">
    </br>
    Phone :<input type="text" name="phone" id="phone" value="<%=contactUpdate.phone%>">
    </br>
    <button type="submit">Update</button>
    </form>

