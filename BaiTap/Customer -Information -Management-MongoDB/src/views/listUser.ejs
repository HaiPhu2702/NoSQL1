<!doctype html>
<html lang="en">
<head>

    <title>listUser</title>

</head>
<body>

<% listUser.forEach((user, index)=>{ %>

<h1>Name :<%= user.name %>
    %>
</h1>
<h2>Code :<%= user.code %>
    %>
</h2>
<h3>email :<%= user.email %> </h3>
<h4>phone :<%= user.phone %>
    %>
</h4>
<button ><a href="/user/update/<%= user.id %>">update</a></button>
<button ><a href="/user/delete/<%= user.id %>">delete</a></button>


<% }) %>

</body>
</html>

